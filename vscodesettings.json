{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Hackerman",
    "editor.fontLigatures": true,
    "editor.fontFamily": "Hack",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 150,
    "files.maxMemoryForLargeFilesMB": 4096,
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        },
    },
    "python.formatting.provider": "black",
    "python.linting.pylintArgs": [
        "--disable=all",
        "--enable=W0614"
    ],
    "workbench.startupEditor": "newUntitledFile",
    "editor.formatOnSave": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "python.sortImports.args": [],
    "python.pythonPath": "C:\\miniconda3\\envs\\shap\\python.exe",
    "todohighlight.include": [
        "**/*.py",
        "**/*.md",
    ],
    // "todohighlight.defaultStyle": ""
    "window.zoomLevel": -1,
    "editor.suggestSelection": "first",
    // "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    // "python.jediEnabled": True,
    "latex-workshop.latex.bibDirs": [
        "C:\\Users\\hiltonc\\Desktop\\readmit\\references\\readmitccf.bib"
    ],
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.intellisense.package.enabled": true,
    // "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latex.clean.subfolder.enabled": true,
    // Defining custom colors instead of default "Rainbow" for dark backgrounds.
    // This is a gradation of cyan. Pretty cute.
    // Restart editor to see changes.
    "indentRainbow.colors": [
        "rgba(0,255,255,0.05)",
        "rgba(0,255,255,0.15)",
        "rgba(0,255,255,0.2)",
        "rgba(0,255,255,0.25)",
        "rgba(0,255,255,0.3)",
        "rgba(0,255,255,0.35)",
        "rgba(0,255,255,0.4)",
    ],
    // The indent color if the number of spaces is not a multiple of "tabSize".
    // "indentRainbow.errorColor": "rgba(128,32,32,0.6)"
    "indentRainbow.errorColor": "",
    // The indent color when there is a mix between spaces and tabs.
    // To be disabled this coloring set this to an empty string.
    "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
    "latex-workshop.latex.tools": [
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        },
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "lualatex",
            "tools": [
                "lualatex"
            ]
        },
        {
            "name": "xe->bib->xe->xe",
            "tools": [
                "xelatex",
                "biber",
                "xelatex",
                "xelatex"
            ]
        },
        {
            "name": "lua->biber->lua->lua",
            "tools": [
                "lualatex",
                "biber",
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex"
            ]
        }
    ],
    "latex-workshop.view.pdf.viewer": "external",
    "latex-workshop.view.pdf.external.command": { // **********
        "command": "C:/Program Files/SumatraPDF/SumatraPDF.exe", // 注意修改路径
        "args": [ // **********
            "%PDF%"
        ]
    },
    "latex-workshop.view.pdf.external.synctex": { // **********
        "command": "C:/Program Files/SumatraPDF/SumatraPDF.exe", // 注意修改路径
        "args": [ // **********
            "-forward-search",
            "%TEX%",
            "%LINE%",
            "%PDF%"
        ]
    },
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.synctex.path": "C:\\texlive\\2018\\bin\\win32\\synctex.exe",
    // "python.dataScience.sendSelectionToInteractiveWindow": true
    // Tanjim Neon colors:     
    // # F602D3 pink 246, 2, 211
    // # 499AE9 blue 73, 154, 233
    // LaTeX
    "vim.useSystemClipboard": true,
    